{
    "version": 2,
    "builds": [
      {
        "src": "frontend/PxAng/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/PxAng/dist/px-ang/browser"
        }
      },
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(products|users|orders|payments)(.*)",
        "dest": "/backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  