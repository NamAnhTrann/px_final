{
    "version": 2,
    "builds": [
      {
        "src": "frontend/PxAng/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/PxAng/dist/PxAng"
        }
      },
      {
        "src": "backend/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "backend/server.js" },
      { "src": "/(.*)", "dest": "frontend/PxAng/dist/PxAng/index.html" }
    ]
  }
  